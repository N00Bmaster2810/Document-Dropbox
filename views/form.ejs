    <div class="container my-4" style="border: 1px solid #cecece">
      <form method="POST" action="/student/submit" enctype="multipart/form-data">
        <table class="table table-striped">
          <thead>
            <tr>
              <th scope="col">S.NO</th>
              <th scope="col">Document type</th>
              <th scope="col">Action</th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <th scope="row">1</th>
              <td>Domicile Certificate</td>
              <td>
                <div class="form-group mb-3">
                  <label for="domicile" class="form-label"></label>
                  <input class="form-control" type="file" id="domicile" name="domicile"  />
                </div>
              </td>
            </tr>
            <tr>
              <th scope="row">2</th>
              <td>Student Photograph</td>
              <td>
                <div class="form-group mb-3">
                  <label for="photograph" class="form-label"></label>
                  <input class="form-control" type="file" id="photograph" name="photograph" />
                </div>
              </td>
            </tr>
            <tr>
              <th scope="row">3</th>
              <td>Income Certificate</td>
              <td>
                <div class="form-group mb-3">
                  <label for="income" class="form-label"></label>
                  <input class="form-control" type="file" id="income" name="income" />
                </div>
              </td>
            </tr>
            <tr>
              <th scope="row">4</th>
              <td>Academic Marksheet</td>
              <td>
                <div class="form-group mb-3">
                  <label for="marksheet" class="form-label"></label>
                  <input class="form-control" type="file" id="marksheet" name="marksheet" />
                </div>
              </td>
            </tr>
            <tr>
              <th scope="row">5</th>
              <td>Aadhar Card</td>
              <td>
                <div class="form-group mb-3">
                  <label for="adhaar" class="form-label"></label>
                  <input class="form-control" type="file" id="adhaar" name="adhaar" />
                </div>
              </td>
            </tr>

            <tr>
              <th scope="row">6</th>
              <td>Bank Passbook</td>
              <td>
                <div class="form-group mb-3">
                  <label for="passbook" class="form-label"></label>
                  <input class="form-control" type="file" id="passbook" name="passbook" />
                </div>
              </td>
            </tr>
            <tr>
              <th scope="row">7</th>
              <td>Bonafide Certificate</td>
              <td>
                <div class="form-group mb-3">
                  <label for="bonafide" class="form-label"></label>
                  <input class="form-control" type="file" id="bonafide" name="bonafide" />
                </div>
              </td>
            </tr>
            <tr><th scope="row"><button type="submit" class="btn btn-secondary btn-lg">Save</button></th></tr>
            
          </tbody>
        </table>
        
      </form>      
